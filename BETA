local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
local Window = Fluent:CreateWindow({
    Title = "• WinterClaws | -BETA- | GEFs [ Basement ] •",
    SubTitle = "By rechedmcvn",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})
local wait = task.wait
local pcall = task.pcall
local spawn = task.spawn
wait(0.25)
local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "layers" }),
    Players = Window:AddTab({ Title = "Players", Icon = "user" }),
    Exploits = Window:AddTab({ Title = "Exploits", Icon = "book" }),
    Visual = Window:AddTab({ Title = "Visual", Icon = "camera" }),
}
function Notify(title, desc, time)
Fluent:Notify({
Title = title,
Content = desc,
Duration = time
})
end
wait(0.25)
Tabs.Main:AddButton({
Title = "Teleport Money",
Callback = function()
for _, v in pairs(workspace:GetDescendants()) do
if v.Name == "Money" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Pickups.Money.CFrame
wait(0.2)
fireproximityprompt(v.ProximityPrompt)
end
end
end})
Tabs.Main:AddToggle("MyToggle",{
Title = "Invisible Planks",
Default = false,
Callback = function(v)
if v then
for _, v in  pairs(workspace:GetDescendants()) do
if v.Name == "Plank" then
v.Transparency = 1
v.CanCollide = false
end
end
InvisPlank = workspace.Planks.ChildAdded:Connect(function(v)
if v.Name == "Plank" then
v.Transparency = 1
v.CanCollide = false
end
end)
else
InvisPlank:Disconnect()
for _, v in  pairs(workspace:GetDescendants()) do
if v.Name == "Plank" then
v.Transparency = 0
v.CanCollide = true
end
end
end
end})
Tabs.Main:AddSection("Miscellaneous")
Tabs.Main:AddDropdown("MyDropdown",{
Title = "Select Items",
Values = {"GPS", "Lantern", "Hammer", "Bat", "Crowbar", "Handgun", "Shotgun", "Bullets", "Shells", "Medkit", "Food", "Soda"},
Multi = false,
Default = "",
Callback = function(v)
getgenv().SelectItem = v
end})
Tabs.Main:AddButton({
Title = "Teleport to item",
Callback = function()
for _, v in pairs(workspace:GetDescendants()) do
if v.Name == getgenv().SelectItem then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Pickups[getgenv().SelectItem].CFrame
end
end
end})
Tabs.Main:AddSection("Proximity Prompts")
Tabs.Main:AddToggle("InstanceInteract",{
Title = "Instance Interact",
Default = false,
Callback = function(v)
if v then
for _,v in pairs(workspace:GetDescendants()) do
if v:IsA("ProximityPrompt") then
v.HoldDuration = 0
end
end
InstanceInteract = workspace.Pickups.ChildAdded:Connect(function(v)
for _,v in pairs(workspace:GetDescendants()) do
if v:IsA("ProximityPrompt") then
v.HoldDuration = 0
end
end
end)
else
InstanceInteract:Disconnect()
end
end})
Tabs.Main:AddToggle("ThroughInteract",{
Title = "Through Interact",
Default = false,
Callback = function(v)
if v then
for _,v in pairs(workspace:GetDescendants()) do
if v:IsA("ProximityPrompt") then
v.RequiresLineOfSight = false
end
end
ThroughInteract = workspace.Pickups.ChildAdded:Connect(function(v)
for _,v in pairs(workspace:GetDescendants()) do
if v:IsA("ProximityPrompt") then
v.RequiresLineOfSight = false
end
end
end)
else
for _,v in pairs(workspace:GetDescendants()) do
if v:IsA("ProximityPrompt") then
v.RequiresLineOfSight = true
end
end
ThroughInteract:Disconnect()
end
end})
Tabs.Main:AddToggle("RemoteInteract",{
Title = "Remote Interact",
Default = false,
Callback = function(v)
if v then
for _,v in pairs(workspace:GetDescendants()) do
if v:IsA("ProximityPrompt") then
v.MaxActivationDistance = 20
end
end
RemoteInteract = workspace.Pickups.ChildAdded:Connect(function(v)
for _,v in pairs(workspace:GetDescendants()) do
if v:IsA("ProximityPrompt") then
v.MaxActivationDistance = 20
end
end
end)
else
for _,v in pairs(workspace:GetDescendants()) do
if v:IsA("ProximityPrompt") then
v.MaxActivationDistance = 7.5
end
end
RemoteInteract:Disconnect()
end
end})
Tabs.Main:AddSection("Lighting")
Tabs.Main:AddToggle("NoDarknessEffect",{
Title = "Ambient Lighting",
Default = false,
Callback = function(v)
game:GetService("RunService").RenderStepped:Connect(function()
if v then
game.Lighting.Brightness = 0
game.Lighting.FogEnd = 9e9
game.Lighting.GlobalShadows = false
game.Lighting.OutdoorAmbient = Color3.new(1,1,1)
else
game.Lighting.Brightness = 1
game.Lighting.GlobalShadows = true
game.Lighting.FogEnd = 250
game.Lighting.OutdoorAmbient = Color3.new(0,0,0)
end
end)
end})
Tabs.Main:AddToggle("NoFog",{
Title = "Ambient Fogdelete",
Default = false,
Callback = function(v)
pcall(function()
if not game.Lighting:GetAttribute("FogStart") then game.Lighting:SetAttribute("FogStart", game.Lighting.FogStart) end
if not game.Lighting:GetAttribute("FogEnd") then game.Lighting:SetAttribute("FogEnd", game.Lighting.FogEnd) end
game.Lighting.FogStart = v and 0 or game.Lighting:GetAttribute("FogStart")
game.Lighting.FogEnd = v and math.huge or game.Lighting:GetAttribute("FogEnd")
local fog = game.Lighting:FindFirstChildOfClass("Atmosphere")
if fog then
if not fog:GetAttribute("Density") then fog:SetAttribute("Density", fog.Density) end
fog.Density = v and 0 or fog:GetAttribute("Density")
end
end)
end})
Tabs.Players:AddSlider("MySlider", {
Title = "Select Speed",
Default = 16,
Min = 16,
Max = 50,
Rounding = 1,
Callback = function(v)
getgenv().Speed = v
end})
Tabs.Players:AddToggle("MyToggle",{
Title = "Speed",
Default = false,
Callback = function(v)
getgenv().EnabledSpeed = v
game:GetService("RunService").RenderStepped:Connect(function()
if getgenv().EnabledSpeed then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = getgenv().Speed or 16
else
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end
end)
end})
Tabs.Players:AddSlider("FOV",{
Title = "Select Field Of View",
Default = 70,
Min = 70,
Max = 120,
Rounding = 1,
Callback = function(v)
getgenv().FieldOfView = v
end})
Tabs.Players:AddToggle("EnabledFOV",{
Title = "Field Of View",
Default = false,
Callback = function(v)
getgenv().Field = v
game:GetService("RunService").RenderStepped:Connect(function()
if getgenv().Field then
workspace.CurrentCamera.FieldOfView = getgenv().FieldOfView or 70
else
workspace.CurrentCamera.FieldOfView = 70
end
end)
end})
Tabs.Players:AddSection("Playersupport")
Tabs.Players:AddToggle("NoWet",{
Title = "No Acceleration",
Default = false,
Callback = function(v)
getgenv().NoSlow = v
game:GetService("RunService").RenderStepped:Connect(function()
game.Players.LocalPlayer.Character.Head.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.LeftFoot.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.LeftHand.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.LeftLowerArm.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.LeftLowerLeg.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.LeftUpperArm.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.LeftUpperLeg.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.LowerTorso.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.RightFoot.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.RightFoot.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.RightHand.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.RightLowerArm.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.RightLowerLeg.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.RightUpperArm.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.RightUpperLeg.Massless = not getgenv().NoSlow
game.Players.LocalPlayer.Character.UpperTorso.Massless = not getgenv().NoSlow
end)
end})
Tabs.Exploits:AddToggle("AntiGEFs",{
Title = "Anti-GEFs",
Default = false,
Callback = function(v)
if v then
for _,v in pairs(workspace:GetDescendants()) do
if v.Parent.Name == "GEFs" then
v.Hitbox.CanTouch = false
v.Hurtbox.CanTouch = false
v.RootPart.CanTouch = false
end
end
AntiGEFs = workspace.GEFs.ChildAdded:Connect(function(v)
if v.Parent.Name == "GEFs" then
v.Hitbox.CanTouch = false
v.Hurtbox.CanTouch = false
v.RootPart.CanTouch = false
end
end)
else
AntiGEFs:Disconnect()
for _,v in pairs(workspace:GetDescendants()) do
if v.Parent.Name == "GEFs" then
v.Hitbox.CanTouch = true
v.Hurtbox.CanTouch = true
v.RootPart.CanTouch = true
end
end
end
end})
