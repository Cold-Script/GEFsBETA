local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
local Window = Fluent:CreateWindow({
    Title = "• WinterClaws | -BETA- | GEFs [ Basement ] •",
    SubTitle = "By rechedmcvn",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})
local wait = task.wait
local pcall = task.pcall
local spawn = task.spawn
wait(0.25)
local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "layers" }),
    Players = Window:AddTab({ Title = "Players", Icon = "user" }),
    Exploits = Window:AddTab({ Title = "Exploits", Icon = "book" }),
    Visual = Window:AddTab({ Title = "Visual", Icon = "camera" }),
}
function Notify(title, desc, time)
Fluent:Notify({
Title = title,
Content = desc,
Duration = time
})
end
wait(0.25)
Tabs.Main:AddToggle("MyToggle",{
Title = "Auto Money Farm",
Default = false,
Callback = function(v)
getgenv().AutoMoneyFarm = v
pcall(function()
spawn(function()
while wait(1) do
if getgenv().AutoMoneyFarm then
for _, v in pairs(workspace:GetDescendants()) do
if v.Name == "Money" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Pickups.Money.CFrame
wait(0.2)
fireproximityprompt(v.ProximityPrompt)
end
end
end
end
end)
end)
end})
Tabs.Main:AddToggle("MyToggle",{
Title = "Invisible Planks",
Default = false,
Callback = function(v)
getgenv().InvisPlank = v
pcall(function()
spawn(function()
while wait(1) do
if getgenv().InvisPlank then
workspace.Planks.Plank.CanCollide = false
workspace.Planks.Plank.Transparency = 1
else
workspace.Planks.Plank.CanCollide = true
workspace.Planks.Plank.Transparency = 0
end
end
end)
end)
end})
Tabs.Main:AddSection("Miscellaneous")
Tabs.Main:AddDropdown("MyDropdown",{
Title = "Select Items",
Values = {"GPS", "Lantern", "Hammer", "Bat", "Crowbar", "Handgun", "Shotgun", "Bullets", "Shells", "Medkit", "Food", "Soda"},
Multi = false,
Default = "",
Callback = function(v)
getgenv().SelectItem = v
end})
Tabs.Main:AddButton({
Title = "Teleport to item",
Callback = function()
for _, v in pairs(workspace:GetDescendants()) do
if v.Name == getgenv().SelectItem then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Pickups[getgenv().SelectItem].CFrame
wait(0.2)
fireproximityprompt(v.ProximityPrompt)
end
end
end})
